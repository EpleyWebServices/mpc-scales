<script>
  var scaleName = ""
  // cl = collapsible list that ignores ajax request to server
  $(document).ready(() => {
    $('a.cl').click(function (event) {
      loadedPadArray.length = 0; // clear all elements from pad load array
      event.preventDefault();
      event.stopPropagation();
      var a_href = $(this).attr('href');
      $('#ajax-content').load(a_href + ' #ajax-content', () => { // cb fires once ajax has loaded
        $(selectedBankButtonId).trigger('click');
        scaleName = unescape(a_href.split('/')[2]);
        document.getElementById('selectedScale').innerHTML = scaleName;
      });
    });
  });
</script>

